{
  "name": "fntoys",
  "version": "1.0.0",
  "description": "fnToys：函数玩具=>一些日常运用频繁的简单函数，因为运用的多且代码量大，所以称为函数玩具",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache",
    "build": "rollup --config && npm run doc",
    "doc": "typedoc --options typedoc.json && npm run doc:gh-fix",
    "doc:gh-fix": "echo test> docs/.nojekyll"
  },
  "keywords": [
    "iwowen",
    "utils",
    "lodash",
    "es",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.2.1",
    "jest": "^29.7.0",
    "rollup": "^2.33.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.4",
    "tslib": "^2.6.3",
    "typedoc": "^0.26.5",
    "typedoc-neo-theme": "^1.1.1",
    "typescript": "^5.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
